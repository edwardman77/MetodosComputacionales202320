    "def MCD(a,b):\n",
    "    \n",
    "    a = np.abs(a)\n",
    "    b = np.abs(b)\n",
    "    \n",
    "    if b > a:\n",
    "        a,b = b,a\n",
    "        \n",
    "    while b:\n",
    "        print(a,b)\n",
    "        a,b = b, a%b\n",
    "        \n",
    "    return a"
   ]
